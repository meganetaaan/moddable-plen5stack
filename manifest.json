{
  "include": [
    "$(MODDABLE)/examples/manifest_base.json",
    "$(MODDABLE)/examples/manifest_piu.json",
    "$(MODDABLE)/modules/pins/digital/manifest.json",
    "$(MODDABLE)/modules/pins/i2c/manifest.json"
  ],
  "modules": {
    "*": ["$(MODDABLE)/modules/drivers/neopixel/*", "./*"]
  },
  "resources": {
    "*-alpha": ["$(MODDABLE)/examples/assets/fonts/OpenSans-Regular-16"]
  },
  "preload": ["neopixel"],
  "platforms": {
    "esp32": {
      "modules": {
        "*": "$(MODULES)/drivers/neopixel/esp32/*"
      }
    },
    "...": {
      "error": "unsupported platform"
    }
  }
}
